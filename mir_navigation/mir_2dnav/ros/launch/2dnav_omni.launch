<?xml version="1.0"?>
<launch>

  <!-- common navigation launch files -->
  <include file="$(find mir_2dnav)/ros/launch/nav_common.launch" />

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" />
	  <rosparam file="$(find mir_2dnav)/ros/config/omni/move_base_params.yaml" command="load" />
		<rosparam file="$(find mir_2dnav)/ros/config/omni/costmap_common_params.yaml" command="load" ns="global_costmap"/>
		<rosparam file="$(find mir_2dnav)/ros/config/omni/costmap_common_params.yaml"	command="load" ns="local_costmap" />
		<rosparam file="$(find mir_2dnav)/ros/config/omni/local_costmap_params.yaml" command="load" />
		<rosparam file="$(find mir_2dnav)/ros/config/omni/global_costmap_params.yaml" command="load" />
		<rosparam file="$(find mir_2dnav)/ros/config/omni/dwa_local_planner_params.yaml" command="load" ns="DWAPlannerROS" />
	</node>

</launch>
